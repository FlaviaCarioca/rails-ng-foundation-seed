<!DOCTYPE html>
<html ng-app="pspApp">
<head>
  <title>ProsperRails</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
</head>
<body>
<h2>jsPageModule:
<%= @jsPageModule %>
</h2>
<%= yield %>


<%= javascript_include_tag 'manifests/lib_man' %>
<%= javascript_include_tag "manifests/#{@jsPageModule}_man" %>
<script>
(function(angular){
  angular.module('pspApp',['<%= @jsPageModule %>'])
      .run(function($rootScope, $templateCache){
          $rootScope.$apply($(document).foundation());
      }
  );
})(angular);
</script>

</body>
</html>
